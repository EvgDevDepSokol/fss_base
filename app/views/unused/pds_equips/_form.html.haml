= form_for @pds_equip do |f|
  - if @pds_equip.errors.any?
    #error_explanation
      %h2= "#{pluralize(@pds_equip.errors.count, "error")} prohibited this pds_equip from being saved:"
      %ul
        - @pds_equip.errors.full_messages.each do |msg|
          %li= msg

  .field
