= form_for @sign_rpt do |f|
  - if @sign_rpt.errors.any?
    #error_explanation
      %h2= "#{pluralize(@sign_rpt.errors.count, "error")} prohibited this sign_rpt from being saved:"
      %ul
        - @sign_rpt.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :engID
    = f.number_field :engID
  .field
    = f.label :engName
    = f.text_field :engName
  .field
    = f.label :BlobObj
    = f.number_field :BlobObj
  .actions
    = f.submit 'Save'
