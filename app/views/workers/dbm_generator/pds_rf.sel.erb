<% data.each do |pds_rf| %>
<%= "/#{mod} #{pds_rf.var2(pds_rf.type)}r#{pds_rf.system.System.tr('/', '_').downcase}#{pds_rf.name.downcase}" %> 
<%= pds_rf.desc12(is_rus) %><%= pds_rf.desc0 %> 
<%= pds_rf.desc1 %> 
<%= pds_rf.desc2 %> 
@<%= pds_rf.type_r?(pds_rf.type) ? 'LIM' : 'IDX' %>(<%= pds_rf.range.tr(';', ',') %>)
@PTAG(<%= (pds_rf.Ptag? ? pds_rf.Ptag : pds_rf.name).upcase %>) 
.UNIT <%= pds_rf.unit_for_dbm(is_rus) %> 
.SYS <%= pds_rf.system.System.tr('/', '_').downcase %> 
.TYPE <%= pds_rf.selection_type(pds_rf.type) %> 
.FORM <%= pds_rf.selection_form(pds_rf.type) %> 
.VALU <%= pds_rf.selection_valu(pds_rf.type) %> 
.DIM 
.PRED <%= predecessor %> 

<% end %>
