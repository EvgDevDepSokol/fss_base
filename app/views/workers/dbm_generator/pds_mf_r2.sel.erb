.TYPE L,1
.FORM L1 
.VALU F,V
.DIM 
.PRED <%= "yp:m#{pds_mf.system.System.downcase}#{format('%02d', pds_mf.Numb)},B,#{pds_mf.number_by_letter(pds_mf_dim.Character)}" %>

<%= "/#{mod} ypxm#{pds_mf.system.System.downcase}#{format('%02d', pds_mf.Numb)}#{pds_mf_dim.Character.downcase}" %> 
