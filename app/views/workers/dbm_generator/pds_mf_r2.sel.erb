.FORM E13.5 
.VALU 0.,V
.DIM 
.PRED <%= "ypxm#{pds_mf.system.System.downcase}#{format('%02d', pds_mf.Numb)},B,#{pds_mf.number_by_letter(pds_mf_dim.Character)*4}" %>

<% end %>
