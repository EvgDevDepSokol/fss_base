<%= "/#{mod} z#{global}#{hw_ic.ref.downcase}" %> 
<%= hw_ic.desc12(is_rus) %><%= hw_ic.desc0 %> 
<%= hw_ic.desc1 %> 
<%= hw_ic.desc2 %> 
<%= gen_tag ? '@EXCL(OR) ' : ''%>
<%= "@SID(#{hw_ic.ref.downcase}) @PANEL(#{hw_ic.pds_panel.panel.downcase}) @PED(#{hw_ped.ped})" %>
<%= "@PTAG(#{hw_ic.ref.upcase})" %><%= (defined? obj.range) ? " @IDX(#{obj.range})" : '' %>

.UNIT <%= hw_ic.unit_for_dbm(is_rus) %>
.SYS <%= obj.system.System.downcase %>
.TYPE L,1
.FORM L1 
.VALU F,V
.DIM <%= dim > 1 ? "(#{dim})" : '' %>
.PRED global<%= global %>

<% if gen_tag && hw_ic.tag_no %>
<%= "/#{mod} z#{global}#{hw_ic.tag_no.downcase}" %> 
<%= hw_ic.desc12(is_rus) %><%= hw_ic.desc0 %> 
<%= hw_ic.desc1 %> 
<%= hw_ic.desc2 %> 
<%= gen_tag ? '@EXCL(OR) ' : ''%>
<%= "@SID(#{hw_ic.ref.downcase}) @PANEL(#{hw_ic.pds_panel.panel.downcase}) @PED(#{hw_ped.ped})" %>
<%= "@PTAG(#{hw_ic.tag_no.upcase})" %><%= (defined? obj.range) ? " @IDX(#{obj.range})" : '' %>

.UNIT <%= hw_ic.unit_for_dbm(is_rus) %>
.SYS <%= obj.system.System.downcase %>
.TYPE L,1
.FORM L1 
.VALU F,V
.DIM <%= dim > 1 ? "(#{dim})" : '' %>
.PRED <%= "z#{global}#{hw_ic.ref.downcase}" %>

<% end %>

<% if ((gen_tag && hw_ic.tag_no)||(!gen_tag)) %>
<% hw_iosignals.each_with_index do |hw_iosignal, index| %>
<%= "/#{mod} z#{global}"%><%=(gen_tag ? hw_ic.tag_no : hw_ic.ref).downcase%><%= hw_iosignal.comment ? "_#{hw_iosignal.comment.downcase}" : ""%> 
<%= hw_ic.desc12_io(is_rus, hw_iosignal.description) %><%= hw_ic.desc0 %> 
<%= hw_ic.desc1 %> 
<%= hw_ic.desc2 %> 
<%= gen_tag ? '@EXCL(OR) ' : ''%>
<%= "@SID(#{hw_ic.ref.downcase}) @PANEL(#{hw_ic.pds_panel.panel.downcase}) @PED(#{hw_ped.ped})" %>
<%= "@PTAG(#{(gen_tag ? hw_ic.tag_no : hw_ic.ref).upcase})" %><%= (defined? obj.range) ? " @IDX(#{obj.range})" : '' %>

.UNIT <%= hw_ic.unit_for_dbm(is_rus) %>
.SYS <%= obj.system.System.downcase %>
.TYPE L,1
.FORM L1 
.VALU F,V
.DIM <%= dim > 1 ? "(#{dim})" : '' %>
.PRED <%= "z#{global}#{(gen_tag ? hw_ic.tag_no : hw_ic.ref).downcase},B,#{index}" %>

<% end %>
<% end %>
